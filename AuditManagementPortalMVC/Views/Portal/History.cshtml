@model IEnumerable<AuditManagementPortalMVC.Models.AuditResponse>

@{
    ViewData["Title"] = "History";
}

<h1>History</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AuditId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjectExexutionStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RemedialActionDuration)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AuditId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ProjectExexutionStatus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RemedialActionDuration)
                </td>

            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="Home">Back to Home</a>
</div>

